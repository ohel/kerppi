<UserControl x:Class="Kerppi.Views.Tasks"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:vms="clr-namespace:Kerppi.ViewModels"
             mc:Ignorable="d"
             d:DesignHeight="400"
             d:DesignWidth="800">
    <UserControl.Resources>
        <vms:NullToBooleanConverter x:Key="nullToBooleanConverter"/>
        <vms:ConverterChainConverter x:Key="nullToVisibilityConverter">
            <vms:NullToBooleanConverter/>
            <vms:BooleanToVisibilityConverter/>
        </vms:ConverterChainConverter>
    </UserControl.Resources>

    <Grid>

    </Grid>
</UserControl>
